{
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "printWidth": 120,
          "tabWidth": 4,
          "useTabs": true,
          "singleQuote": false,
          "bracketSpacing": false
        }
      },
      {
        "files": ["*.ts", "*.js"],
        "options": {
          "printWidth": 140,
          "tabWidth": 4,
          "useTabs": true,
          "singleQuote": false,
          "bracketSpacing": true
        }
      }
    ]
  }